<div class="well">
  <h1><%= t('.title')%></h1>
  <p><%= t('.description')%></p>
</div>
<hr />
<div class="table-responsive">
  <table class="table table-striped table-hover table-bordered table-condensed">
    <thead>
      <tr>
        <th><%= School.human_attribute_name('name')%></th>
        <th><%= Course.human_attribute_name('active_students')%></th>
      </tr>
    </thead>
    <tbody>
      <% @results.each do |result|%>
        <tr>
          <td><%= result.name%></td>
          <td><%= result.total%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
